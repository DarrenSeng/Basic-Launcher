{
    "manifest_version": 3,
    "name": "Darren's Launcher",
    "description": "Quick launch for my school-related sites.",
    "version": "1.0.0",
    "icons": {
        "128": "images/icon_128.png"
    },
    "action": {
        "default_icon": "images/icon.jpg",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}
